    "eslint": "^8.57",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.9",
    "eslint-plugin-prettier": "^4",
    "fake-indexeddb": "5.0.1",
    "html-webpack-plugin": "^5.6.0",
    "jest": "29.7",
    "jest-fetch-mock": "^3.0",
    "lerna": "^8",
    "react": "^18.2",
    "react-dom": "^18.2",
    "react-test-renderer": "18.2",
    "svelte-jester": "3.0",
