{
  "name": "@ic-reactor/core",
  "version": "3.0.9-beta.0",
  "description": "IC Reactor Core Library",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "rm -rf dist tsconfig.tsbuildinfo && tsc -b",
    "test": "vitest run",
    "version:sync": "node -e \"const pkg = require('./package.json'); const fs = require('fs'); fs.writeFileSync('./src/version.ts', '/**\\n * Library version - automatically synced from package.json.\\n */\\nexport const VERSION = \\\"' + pkg.version + '\\\"\\n');\"",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/B3Pay/ic-reactor",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/B3Pay/ic-reactor/issues"
  },
  "homepage": "https://ic-reactor.dev",
  "keywords": [
    "internet-computer",
    "icp",
    "dfinity",
    "web3",
    "blockchain",
    "canister",
    "actor",
    "reactor",
    "tanstack-query"
  ],
  "author": "Behrad Deylami",
  "license": "MIT",
  "dependencies": {
    "@noble/curves": "^2.0.1",
    "zod": "^4.3.6"
  },
  "peerDependencies": {
    "@icp-sdk/core": "^5.0.0",
    "@icp-sdk/auth": "^5.0.0",
    "@tanstack/query-core": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "@icp-sdk/auth": {
      "optional": true
    },
    "@tanstack/query-core": {
      "optional": true
    }
  },
  "devDependencies": {
    "@icp-sdk/auth": "^5.0.0",
    "@icp-sdk/core": "^5.0.0",
    "@size-limit/preset-small-lib": "^12.0.0",
    "@tanstack/query-core": "^5.90",
    "@tanstack/react-query": "^5.90",
    "@types/node": "^25.0.10",
    "fake-indexeddb": "^6.2.5",
    "size-limit": "^12.0.0",
    "vitest": "^4.0.18"
  },
  "size-limit": [
    {
      "name": "Core Library",
      "path": "dist/index.js",
      "limit": "50 KB",
      "gzip": true
    }
  ]
}
