{
  "name": "@ic-reactor/vite-plugin",
  "version": "0.1.0",
  "description": "Vite plugin for zero-config IC reactor generation from Candid files",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --tsconfig tsconfig.json",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch --tsconfig tsconfig.json"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "internet-computer",
    "candid",
    "ic-reactor",
    "dfinity",
    "icp"
  ],
  "author": "Behrad Deylami",
  "license": "MIT",
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0"
  },
  "dependencies": {
    "chokidar": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.9.0",
    "vite": "^6.0.0"
  }
}
